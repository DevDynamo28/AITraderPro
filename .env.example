# Example .env file for Agentic AI Trader
# Copy this file to .env and fill in your credentials

# OpenAI API Key (required for AI analysis and reflection)
OPENAI_API_KEY=your_openai_api_key_here

# MetaTrader 5 credentials (optional if MT5 is already logged in)
# These are only needed if your MT5 terminal is not already logged in
MT5_SERVER=your_mt5_server_address_here
MT5_LOGIN=your_mt5_login_here
MT5_PASSWORD=your_mt5_password_here

# Optional environment variables
SESSION_SECRET=your_session_secret_here
