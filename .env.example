# Example .env file for Agentic AI Trader
# Copy this file to .env and fill in your credentials

# OpenAI API Key (required for AI analysis and reflection)
OPENAI_API_KEY=your_openai_api_key_here

# MetaTrader 5 credentials (required for live trading)
MT5_API_URL=https://your_mt5_api_server.com
MT5_SERVER=your_mt5_server_address_here
MT5_LOGIN=your_mt5_login_here
MT5_PASSWORD=your_mt5_password_here
MT5_API_KEY=your_mt5_api_key_here  # Optional, some MT5 API servers require an API key

# Optional environment variables
SESSION_SECRET=your_session_secret_here
